    <servlet-name>DataCreate</servlet-name>
    <url-pattern>/DataCreate.do</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>CompanyDataDetails</servlet-name>
    <servlet-class>com.data.CompanyDataDetails</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>CompanyDataDetails</servlet-name>
    <url-pattern>/CompanyDataDetails.do</url-pattern>
  </servlet-mapping>
  <servlet>
  		<servlet-name>ContactsUpdate</servlet-name>
  		<servlet-class>com.data.ContactsUpdate</servlet-class>
  </servlet>
  <servlet-mapping>
  		<servlet-name>ContactsUpdate</servlet-name>
  		<url-pattern>/ContactsUpdate.do</url-pattern>
  </servlet-mapping>
</web-app>